     <FULL_NAME>\PR:CL_WDR_VALUE_HELP_HANDLER=====CP\TY:LCL_DDIC_SUGGEST_HANDLER_2\ME:GET_FIELD_LABEL\SE:BEGIN\EI</FULL_NAME>
     <SOURCE>
      <item>* bug fix.</item>
      <item>  READ TABLE m_shlp-fielddescr INTO data(zl_dfies) WITH KEY fieldname = component-name.</item>
      <item>  IF sy-subrc EQ 0.</item>
      <item>    clear field_label.</item>
      <item>    field_label = zl_dfies-scrtext_m.</item>
      <item>    if field_label is initial.</item>
      <item>      field_label = zl_dfies-scrtext_l.</item>
      <item>      if field_label is initial.</item>
      <item>        field_label = zl_dfies-scrtext_s.</item>
      <item>        if field_label is initial.</item>
      <item>          field_label = zl_dfies-reptext.</item>
      <item>        endif.</item>
      <item>      endif.</item>
      <item>    endif.</item>
      <item>    IF field_label IS NOT INITIAL.</item>
      <item>      RETURN.</item>
      <item>    ENDIF.</item>
      <item>  ENDIF.</item>
     </SOURCE>
    </ENH_HOOK_IMPL>
